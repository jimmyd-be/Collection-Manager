{
    "name": "lonelobo0070/collection-manager-backend",
    "description": "Collection manager backend",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "https://github.com/lonelobo0070/Collection-Manager-Backend",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "doctrine/orm": "^2",
        "imdbphp/imdbphp": "^6.3",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "rbdwllr/reallysimplejwt": "^2.0",
        "slim/psr7": "dev-master",
        "slim/slim": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8080 -t public",
        "test": "phpunit"
    }
}
